{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rulesDirectory": [
    "node_modules/tslint-origin-ordered-imports-rule/dist"
  ],
  "rules": {
    "origin-ordered-imports": [true],
    "quotemark": [true, "single"],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
  }
}